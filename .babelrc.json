{
  "presets": ["babel-preset-gatsby"],
  "plugins": [
    "@babel/plugin-transform-destructuring",
    "babel-plugin-styled-components",
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@components": "./src/components",
          "@resources": "./src/resources",
          "@styles": "./src/styles"
        }
      }
    ]
  ]
}
